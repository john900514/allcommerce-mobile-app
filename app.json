{
  "expo": {
    "name": "AllCommerce",
    "slug": "allcommerce",
    "privacy": "public",
    "sdkVersion": "36.0.0",
    "owner": "capeandbay",
    "platforms": [
      "ios",
      "android"
    ],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "cover",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.capeandbay.allcommerce"
    },
    "android": {
      "package": "com.capeandbay.allcommerce"
    },
    "packagerOpts": {
      "sourceExts": ["js", "json", "ts", "tsx", "vue"],
      "config": "metro.config.js"
    }
  }
}
